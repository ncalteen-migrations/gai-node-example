{
  "name": "@ncalteen-migrations/gai-node-example",
  "version": "2.0.0",
  "description": "Functions for working with the Fibonacci sequence",
  "main": "index.js",
  "scripts": {
    "coverage": "jest --ci --color --verbose --coverage",
    "test": "jest --ci --color --verbose --json --outputFile=reports/jest.json --reporters=default --reporters=jest-junit",
    "all": "npm run coverage && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ncalteen-migrations/gai-node-example.git"
  },
  "author": "Bryan Cross <bryancross@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ncalteen-migrations/gai-node-example/issues"
  },
  "homepage": "https://github.com/ncalteen-migrations/gai-node-example#README.md",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "coverageDirectory": "<rootDir>/reports",
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "<rootDir>/reports",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.1.2"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0"
  }
}
